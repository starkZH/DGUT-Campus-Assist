page{
  width: 100%;
  height: 100%;
  background-color: white;
}
.panel{
  display: flex;
  flex-direction: column;
  margin-top: 5%;
  box-shadow: 0px 0px 10px lightgray;
  border-radius: 8px;
  margin-left: 3%;
  width: 94%;
  background-color: white;
}
.subpanel{
  display: flex;
  flex-direction: row;
  border-bottom: 2px dotted lightgray;
  margin-top: 3%;
  margin-left: 3%;
  width: 94%;
}
.titlePanel{
  display: flex;
  flex-direction: row;
  margin-top: 5%;
}
.tsub,.subject{
  width: 70%;
  padding-left: 3%;
}
.txf,.xf{
width: 20%;
text-align: center; 
}
.tsco,.score{
width: 20%;

}
.xq{
  margin-top: 2%;
  margin-left: 2%;
  font-size: 20px;
}