page{
  width: 100%;
  height: 100%;
}
.base{
width: 100%;
height: 100%;
position: relative;
color: white;
}
.bgImage{
width: 100%;
height: 100%;
position: absolute;
top: 0px;
z-index: -100
}
.yetext,.last{
  color: white;
  width: 100%;
  text-align: center;
  position: absolute;
}
.yetext{
  margin-top: 5%;
  font-size: 42rpx;
}
.last{
  margin-top: 15%;
  font-size: 50rpx;
}
.t2,.v2{
position: absolute;
margin-top: 40%;
margin-left: 5%;
}
.t3,.v3{
position: absolute;
margin-top: 60%;
margin-left: 5%;
}
.t2,.t3{
  font-size: 36rpx;
  width: 50%;
  text-align: center;
}
.v2,.v3{
  font-size: 40rpx;
  margin-left:50%;
  width: 50%;
  text-align: center;
}
.tip{
  font-size: 32rpx;
  margin-left: 5%;
  width: 90%;
  position: fixed;
  bottom: 20%;
  text-align: center;
  color: white;
}