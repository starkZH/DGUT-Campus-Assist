page{
  width: 100%;
  height: 100%;
  background-color: #EFEFEF
}
.textPanel{
  background-color: white;
  width:100%;margin-left:0;margin-top:0%;height:35%;
}
.obj{
  width:100%;height:100%;
  border-radius: 2px;
}
.text{
  margin-top: 2%;
  width: 96%;
  height: 75%;
  margin-left: 2%;
  font-size: 33rpx;
}
.input{
margin-left: 36%;
margin-top: 20rpx;
}
.label{
    border: 1px solid;
  color: #FF6461;
  border-radius: 2px;
text-align: center;
font-size: 34rpx;
height: 49rpx;
margin-top: 15rpx;
margin-left: 20rpx;
}
.labels{
  width: 100%;
  height: 75rpx;
}
.upload{
 display: flex;
flex-flow: row wrap;
align-content: space-around;
  background: white;
  border-bottom: 1px solid  rgb(241,241,241);
  background-color: white;
}
.uploadTip{
  position: relative;
  top: -35rpx;
  background-color: rgba(0, 0, 0, 0.5);
  color: white;
  text-align: center;
  font-size: 32rpx;
}
.imagePanel{
  display: flex;
  flex-direction: row;
  height: 100%;
  width: 100%;
  margin-bottom: 3%;
}
.image{
  margin-left: 3%;
  margin-top: 3%;
  width: 20%;
  height: 100%;
  box-shadow: 0px 0px 2px gray;
  box-sizing: border-box;
flex: 0 0 20%;
display: block;
border-radius: 2px;
}
.close{
  width: 40rpx;
  border-radius: 99px;
}
